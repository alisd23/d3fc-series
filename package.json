{
  "name": "d3fc-series",
  "description": "A collection of components for rendering data series to SVG and canvas, including line, bar, OHLC, candlestick and more",
  "license": "MIT",
  "keywords": [
    "d3",
    "d3fc",
    "scale"
  ],
  "homepage": "https://github.com/d3fc/d3fc-series",
  "repository": {
    "type": "git",
    "url": "https://github.com/d3fc/d3fc-series"
  },
  "scripts": {
    "bundle": "rimraf build && rollup -c && uglifyjs build/d3fc-series.js --compress --mangle -o build/d3fc-series.min.js",
    "test": "eslint index.js src/**/*.js test/**/*.js examples/**/*.js && npm run bundle && jasmine JASMINE_CONFIG_PATH=test/support/jasmine.json",
    "site": "rollup -c demo/rollup.config.js"
  },
  "devDependencies": {
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-register": "^6.11.5",
    "d3": "^4.1.1",
    "d3fc-extent": "^2.2.0",
    "d3fc-random-data": "^1.0.0",
    "eslint": "^2.13.1",
    "eslint-config-defaults": "^9.0.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "jasmine": "^2.4.1",
    "jsdom": "^9.4.1",
    "rimraf": "^2.5.2",
    "rollup": "^0.34.1",
    "rollup-plugin-babel": "^2.4.0",
    "uglify-js": "^2.6.2",
    "webshot": "^0.18.0"
  },
  "dependencies": {
    "d3-array": "^1.0.0",
    "d3-scale": "^1.0.1",
    "d3-selection": "^1.0.0",
    "d3-shape": "^1.0.0",
    "d3fc-data-join": "^2.0.0",
    "d3fc-rebind": "^4.0.1",
    "d3fc-shape": "^4.0.0"
  }
}
